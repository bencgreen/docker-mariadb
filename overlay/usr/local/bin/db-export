#!/usr/bin/with-contenv bash

set -euo pipefail


#======================================================================================================================
# Run ${LIB}/mariadb/export executable
#======================================================================================================================

if [ $# -eq 1 ] && [ -n "${1}" ] ; then
    s6-setuidgid dbadm ${LIB}/mariadb/export "${1}"
else
    bcg-error "db-export: You must specify a database name."
fi
